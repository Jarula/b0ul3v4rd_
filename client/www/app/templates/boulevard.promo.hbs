<div class="promotion">
    <div class="promo-title"></div>
    <div class="promo-description"></div>
    <div class="col s12 m7">
        <div class="card">
            <div class="card-image">
                <img src="{{picture}}" height="200">
            </div>
            <div class="card-content">
                <h4>{{title}}</h4>
                <p>{{description}}</p>
            </div>
        </div>
    </div>
</div>